<script>


    function AddToCart(ProductId) {
        var PProductId = parseInt(ProductId);
        $.ajax({
            url: "/Cart/AddToCart/",
            type: "POST",
            contentType: "application/json;charset=utf-8",
            dataType: "json",
            data: JSON.stringify({ ProductId: PProductId}),
            success: function (result) {
                debugger;
                console.log(result.status);
                if (result.status == "success") {
                    Notiflix.Report.Success();
                    $("#NotiflixReportWrap").fadeIn();
                    $(".notiflix-report-title").text("Item added in cart");
                    $(".notiflix-report-message").text("");
                    $(".notiflix-report-overlay").css({ "background": "rgba(255,255,255, 0.7)", "animation-duration": "360ms;" });
                    $("#NXReportButton").hide();
                    mytimer();                    
                }                              
            },
            error: function (errormessage) {
            }
        });

        var timeleft = 1;
        function mytimer() {
            var downloadTimer = setInterval(function () {
                if (timeleft <= 0) {
                    clearInterval(downloadTimer);
                    $("#NotiflixReportWrap").fadeOut();

                } else {

                }
                timeleft -= 1;
            }, 1000);
        }
    }
  
</script>