        @ViewBag.Msg
    </div>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <script src="~/Content/PopUp/notiflix-aio-2.7.0.min.js"></script>

    <script>
        $(document).ready(function () {
            var message = $('.Msgdiv').text().trim();
            if (message == 'Fail') {
                Notiflix.Report.Failure();
                $(".notiflix-report-title").text("Username or password is incorrect");
                $(".notiflix-report-message").text("");
                $(".notiflix-report-overlay").css({ "background": "rgba(255,255,255, 0.7)", "animation-duration": "360ms;" });
                $("#NXReportButton").hide();
                mytimer();
                $('.Msgdiv').text();
            }

            var timeleft = 1;
            function mytimer() {
                var downloadTimer = setInterval(function () {
                    if (timeleft <= 0) {
                        clearInterval(downloadTimer);
                        $("#NotiflixReportWrap").fadeOut();

                    } else {

                    }
                    timeleft -= 1;
                }, 500);
            }
        });


        $(document).ready(function () {
            $("#registerbtn").click(function () {
                var Email = $("#Emailtxt").val();
                var FirstName = $("#Nametxt").val();
                var Password = $("#Passwordtxt").val();
                if (Email == "") {
                    $("#Emailtxt").css('border-color', 'red');
                    $("#Emailtxt").focus();
                }
                else if (FirstName == "") {
                    $("#Nametxt").css('border-color', 'red');
                    $("#Nametxt").focus();
                }
                else if (Password == "") {
                    $("#Passwordtxt").css('border-color', 'red');
                    $("#Passwordtxt").focus();
                }
                else {
                    $.ajax({
                        url: "/Login/Register/",
                        type: "POST",
                        contentType: "application/json;charset=utf-8",
                        dataType: "json",
                        data: JSON.stringify({ Email: Email, FirstName: FirstName, Password: Password }),
                        success: function (result) {
                            if (result.status == "success") {
                                if (result.Message == "success") {
                                    Notiflix.Report.Success();
                                    $(".notiflix-report-title").text("Registered Successfully");
                                    $(".notiflix-report-message").text("");
                                    $(".notiflix-report-overlay").css({ "background": "rgba(255,255,255, 0.7)", "animation-duration": "360ms;" });
                                    $("#NXReportButton").hide();
                                    mytimer();
                                    window.location.href = "https://localhost:44397";
                                }
                                else if (result.Message == "Forget Password") {
                                    Notiflix.Report.Warning();
                                    $(".notiflix-report-title").text("Forgot your password contact admin");
                                    $(".notiflix-report-message").text("");
                                    $(".notiflix-report-overlay").css({ "background": "rgba(255,255,255, 0.7)", "animation-duration": "360ms;" });
                                    $("#NXReportButton").hide();
                                    mytimer();
                                    
                                }
                            }
                        },
                        error: function (errormessage) {
                        }
                    });
                }

            });

            $("#Emailtxt").keyup(function () {
                $(this).css('border-color', 'black');
            });

            $("#Nametxt").keyup(function () {
                $(this).css('border-color', 'black');
            });

            $("#Passwordtxt").keyup(function () {
