    </script>

    <script>
        $(document).ready(function () {
            var max = $("#rangeSliderExample3MaxResult").val();
            var min = $("#rangeSliderExample3MinResult").val();
            $("#minlable").html(min);
            $("#maxlable").html(max);


        });




    </script>
    <script>
        $("#searchname").on("keyup", function () {
            var txtenter = $("#searchname").val();
            $('table tr').each(function (results) {
                if (results !== 0) {
                    var id = $(this).find("td:nth-child(2)").text();
                    if (id.indexOf(txtenter) !== 0 && id.toLowerCase().indexOf(txtenter.toLowerCase()) < 0) {
                        $(this).hide();
                    }
                    else {
                        $(this).show();
                    }
                }
            })
        })
    </script>

