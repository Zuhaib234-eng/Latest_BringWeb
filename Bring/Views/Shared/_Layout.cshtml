        var TrackDetailPostBackURL = '/TrackOrder/Detail';
        $(function () {
            $("#track").click(function () {
                var id = $("#orderid").val();
                $.ajax({
                    type: "GET",
                    url: TrackDetailPostBackURL,
                    contentType: "application/json; charset=utf-8",
                    data: { "Id": id },
                    datatype: "json",
                    success: function (data) {

                        $('#myModalContent').html(data);
                        //var status = $("#status").attr("data:id");

                        if (status == "OrderPlaced") {

                            $("#first").show();
                            $("#second").hide();
                            $("#third").hide();
                        }
                        else if (status == "OnTheWay") {
                            $("#second").show();
                            $("#first").hide();
                            $("#third").hide();
                        }

                        else if (status == "Delivered") {
                            $("#orderHeading").text("Order Delivered ! Thank you for shopping");
                            $("#third").show();
                            $("#first").hide();
                            $("#second").hide();


                        }
                        $('#myModal').modal('show');

                    },
                    error: function () {
                        alert("Failed Tracking");
                    }
                });
            });
        });










